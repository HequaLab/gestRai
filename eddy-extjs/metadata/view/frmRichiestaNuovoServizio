                                    "                                no:'No',",
                                    "                                yes:'Si'",
                                    "                            },",
                                    "                            fn: function(btn){",
                                    "                                if (btn=='no'){",
                                    "                                    alert('premuto no');",
                                    "                                    return;",
                                    "                                }",
                                    "                            }",
                                    "                        });",
                                    "                    }",
                                    "",
                                    "",
                                    "                    store.add(emptyApp);",
                                    "                    var self = this;",
                                    "                    store.on('write', function() {",
                                    "                        store.load();",
                                    "                    });",
                                    "",
                                    "",
